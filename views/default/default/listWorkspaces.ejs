<!-- Default home page -->
<link href='//fonts.googleapis.com/css?family=Open+Sans:400italic,600' rel='stylesheet' type='text/css'>


<div class="default-page">
  <div class="header">
    <h1 id="main-title" class="container"><%= TranslationService.t('workspaces') %></h1>
    <br/>
  </div>
  <div class="row" id="loading">
    <div class="col-xs-1">
    </div>
    <div class="col-xs-10">
      <img class="center-block" src="/images/loading.svg" alt="<%= TranslationService.t('loading-alt-text') %>">
    </div>
    <div class="col-xs-1">
    </div>
  </div>
  <div class="main container clearfix">
    <workspace-list branding="<%= typeof branding == 'undefined'? data.branding : branding %>" portal="<%= typeof portal == 'undefined'? data.portal : portal %>" packageType="workspace"></workspace-list>
    <%
    var currentMinute = sails.config.startupMinute;
    %>
    <link href="/angular/workspace_list/styles.bundle.css?ts=<%= currentMinute %>" rel='stylesheet' type='text/css'>
    <script type="text/javascript" src="/angular/workspace_list/inline.bundle.js?ts=<%= currentMinute %>"></script>
    <script type="text/javascript" src="/angular/workspace_list/polyfills.bundle.js?ts=<%= currentMinute %>"></script>
    <script type="text/javascript" src="/angular/workspace_list/vendor.bundle.js?ts=<%= currentMinute %>"></script>
    <script type="text/javascript" src="/angular/workspace_list/main.bundle.js?ts=<%= currentMinute %>"></script>

  </div>
</div>
